<% current_user.notifications.last(Settings.notify.max_notify_show).reverse.each do |notification| %>
  <%= link_to notification.notification_url do %>
    <div class="notification">
      <%= render "notifications/notification", notification: notification %>
    </div>
  <% end %>
<% end %>
<div>
  <%= link_to t(".all_notify"), notifications_all_path, class: "all-notify-header" %>
</div>
